{
  "name": "tsla-stock-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "preinstall": "npm install vite@5.4.19 --save-exact",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare-deploy": "node deploy.cjs",
    "start": "node backend/server.js",
    "start:backend": "node backend/server.js",
    "start:dev": "concurrently \"npm run dev\" \"nodemon backend/server.js\"",
    "render-build": "npm install --legacy-peer-deps --force && npm run build && npm run prepare-deploy",
    "postinstall": "npm install --production"
  },
  "dependencies": {
    "@vitejs/plugin-react": "4.5.0",
    "autoprefixer": "10.4.18",
    "axios": "1.9.0",
    "date-fns": "3.0.6",
    "esbuild": "0.20.2",
    "html2canvas": "1.4.1",
    "lightweight-charts": "4.1.1",
    "lucide-react": "0.344.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-tabs": "6.0.2",
    "tailwindcss": "3.4.1",
    "vite": "5.4.19",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@eslint/js": "9.9.1",
    "@types/node": "20.11.16",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/react-tabs": "5.0.5",
    "concurrently": "8.2.2",
    "dotenv": "16.5.0",
    "eslint": "9.9.1",
    "eslint-plugin-react-hooks": "5.1.0-rc.0",
    "eslint-plugin-react-refresh": "0.4.11",
    "globals": "15.9.0",
    "postcss": "8.4.35",
    "postcss-import": "15.0.0",
    "postcss-nested": "6.0.1",
    "postcss-preset-env": "9.1.0",
    "typescript": "5.5.3",
    "typescript-eslint": "8.3.0",
    "vite": "5.4.19"
  }
}
