{
  "name": "tsla-stock-dashboard",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "preinstall": "npm install vite@latest --save-exact",
    "dev": "vite",
    "build": "node ./node_modules/vite/bin/vite.js build --config vite.config.js",
    "lint": "eslint .",
    "preview": "npx vite preview",
    "prepare-deploy": "node deploy.cjs",
    "start": "cd backend && node server.js",
    "start:backend": "cd backend && node server.js",
    "start:dev": "concurrently \"npm run dev\" \"npm run start:backend\"",
    "render-build": "npm install && npm run build && npm run prepare-deploy"
  },
  "dependencies": {
    "vite": "^5.4.2",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.9.0",
    "date-fns": "^3.0.6",
    "html2canvas": "^1.4.1",
    "lightweight-charts": "^4.1.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-tabs": "^6.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-tabs": "^5.0.5",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.19"
  }
}
